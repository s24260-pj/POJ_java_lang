package zjazd2;

public class Main {
	public static void main(String[] args) {
		String[] tablica = {"1", "6", "1"};
		System.out.println(sum(tablica));
	}
	
	public static String sum(String[] array) {
		
		int value = 0;
		
        for (String element: array) {
        	Integer number = Integer.valueOf(element);
        	value += number;
        }
		
		return ""+value;
	}
}
